# Key Algorithm
variable key_algorithm {
  description = "The algorithm that should be used for the certificate"
  type = string
}

# Certificate CN
variable certificate_common_name {
  description = "The common name that should be used for the certificate"
  type = string
}

# Certificate Org
variable certificate_organisation {
  description = "The organisation that should be used for the certificate"
  type = string
}
